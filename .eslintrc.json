{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off",
    "import/prefer-default-export": "off",
    "import/extensions": "off",
    "no-plusplus": "off",
    "import/no-extraneous-dependencies": "off"
    //Add other rules if u needed too
  }
}
